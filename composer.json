{
    "name": "palladiumlab/palladiumlab",
    "description": "This is palladiumlab library for bitrix projects",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ilya",
            "email": "ilya@palladiumlab.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "symfony/var-dumper": "^4.3",
        "vlucas/phpdotenv": "^3.5",
        "laravel/helpers": "^1.1",
        "monolog/monolog": "^2.0",
        "symfony/console": "^4.0",
        "symfony2admingenerator/twig-generator": "^1.3",
        "twig/twig": "^2.9",
        "illuminate/events": "^5.0",
        "philo/laravel-blade": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Palladiumlab\\": [
                "commands",
                "src"
            ]
        },
        "files": [
            "local/php_interface/functions.php",
            "local/php_interface/load.php",
            "local/php_interface/const.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Palladiumlab/palladiumlab.git"
        }
    ],
    "bin": ["bin/palladiumlab-console"]
}
